        <%= link_to 'POST UP',new_product_path , :class => "btn btn-info" %>
        <%= link_to 'RENT',products_path, :class => "btn btn-info" %>
        <%= link_to 'BACK', users_path, :class => "btn btn-info" %>
      </div>
      <div class="gearbox">
        <h3 class="text-center lead gear-header">My Garage</h3>
        <button class="btn btn-primary" type="button">Compare Rentals</button>
        <button class="btn btn-primary" type="button">Reviews</button>
        <button class="btn btn-primary" type="button">Past Rentals</button>
        <button class="btn btn-primary" type="button">Log Out</button>

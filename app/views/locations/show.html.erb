<h1>Listing Locations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th> </th>
      <th>email</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location.users.each do |locationuser| %>
      <tr>
        <td><%= locationuser.first_name %></td>
        <td><%= locationuser.last_name %></td>
        <td><%= locationuser.email %></td>
        <td><%= link_to 'Show', locationuser %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
